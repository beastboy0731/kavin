var submittedErrorReports = [];
window.addEventListener('error', function(errorEvent) {
  function getCookie(cookie) {
    var value = '; ' + document.cookie;
    var parts = value.split('; ' + cookie + '=');
    if (parts.length == 2) {
      return parts.pop().split(';').shift();
    } else {
      return '';
    }
  }

  function serializeParam(data) {
    return Object.keys(data).map(function(key) {
      return key + '=' + encodeURIComponent(data[key]);
    }).join('&');
  }

  function formatError(event) {
    var error = event.error || {};
    return {
      name: error.name,
      message: error.message || event.message,
      stack: error.stack,
      filename: event.filename,
      line: event.lineno,
      column: event.colno,
    };
  }

  var reportData;

  try {
    reportData = JSON.stringify({ errorObj: formatError(errorEvent) });
  } catch (e) {
    reportData = JSON.stringify({
      errorObj: { message: e.message },
    });
  }

  var reportBody = serializeParam({
    report_context: JSON.stringify({
      current_url: window.location.href,
      client_version: '46a6916',
    }),
    report_data: reportData,
  });

  var encodedReportBody = window.btoa(reportBody);

  if (window.submittedErrorReports.indexOf(encodedReportBody) === -1) {
    var xhr = new XMLHttpRequest();

    xhr.open('post', '/_/_/report/error/', true);
    xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
    xhr.setRequestHeader('X-CSRFToken', getCookie('csrftoken'));
    xhr.send(reportBody);

    window.submittedErrorReports.push(encodedReportBody);
  }

  if (window.submittedErrorReports.length > 100) {
    window.submittedErrorReports.shift();
  }
});
</script><div id="__PWS_ROOT__" data-reactcontainer="true"><div class="App AppBase"><div class="HeaderContent" data-test-id="header" id="HeaderContent" style="z-index:671"><div data-test-id="header-Header" class="zI7 iyn Hsu"><div class=""><div class="qJc xcv L4E zI7 iyn Hsu"><div class="Nags"><noscript><div class="qiB" style="top:64px;z-index:2"><div class="NagBase"><div class="BsF Jea QDA b8T ccF d5Q gjz hUC jzS zI7 iyn Hsu"><div class="fC1 Wyy xcv L4E zI7 iyn Hsu"><div class="tBJ dyH iFc SMy yTZ erh DrD IZT mWe"><div class="message"><span>Pinterest doesn't work unless you <a style="text-decoration: underline; color: #fff" href="http://enable-javascript.com/" target="_blank">turn on JavaScript.</a></span></div></div></div><div class="Jea gjz jx- zI7 iyn Hsu"></div></div></div></div></noscript></div><div class="Jea jzS zI7 iyn Hsu"><div class="C9q Jea Zr3 ujU xcv L4E zI7 iyn Hsu"><div class="Jea gjz xcv L4E zI7 iyn Hsu" style="height:56px"><div class="zI7 iyn Hsu"><div><div class="XiG zI7 iyn Hsu"><div aria-label="Home" class="zI7 iyn Hsu"><a class="dangerouslyDisableFocusStyle" href="/" rel=""><div class="INd zI7 iyn Hsu" style="background-color:transparent;height:48px;width:48px"><div class="C9q Jea Shl gjz jzS mQ8 zI7 iyn Hsu" style="height:48px;width:48px"><svg class="gUZ lZJ U9O kVc" height="28" width="28" viewBox="0 0 24 24" aria-hidden="true" aria-label="" role="img"><path d="M0 12c0 5.123 3.211 9.497 7.73 11.218-.11-.937-.227-2.482.025-3.566.217-.932 1.401-5.938 1.401-5.938s-.357-.715-.357-1.774c0-1.66.962-2.9 2.161-2.9 1.02 0 1.512.765 1.512 1.682 0 1.025-.653 2.557-.99 3.978-.281 1.189.597 2.159 1.769 2.159 2.123 0 3.756-2.239 3.756-5.471 0-2.861-2.056-4.86-4.991-4.86-3.398 0-5.393 2.549-5.393 5.184 0 1.027.395 2.127.889 2.726a.36.36 0 0 1 .083.343c-.091.378-.293 1.189-.332 1.355-.053.218-.173.265-.4.159-1.492-.694-2.424-2.875-2.424-4.627 0-3.769 2.737-7.229 7.892-7.229 4.144 0 7.365 2.953 7.365 6.899 0 4.117-2.595 7.431-6.199 7.431-1.211 0-2.348-.63-2.73